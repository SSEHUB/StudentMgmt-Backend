{
	"moduleFileExtensions": ["js", "json", "ts"],
	"rootDir": "../",
	"testEnvironment": "node",
	"testMatch": ["<rootDir>/test/e2e/**/*.e2e-spec.ts"],
	"transform": {
		"^.+\\.ts$": "ts-jest"
	},
	"collectCoverageFrom": ["src/**/*.ts", "!src/test"],
	"coveragePathIgnorePatterns": [],
	"setupFilesAfterEnv": ["<rootDir>/test/jest.setup.js"]
}
